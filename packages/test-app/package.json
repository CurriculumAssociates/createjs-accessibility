{
  "name": "@curriculumassociates/accessibility-test-app",
  "version": "2.0.0",
  "description": "A module to allow for easy addition of accessibility when using createjs",
  "keywords": [
    "accessibility",
    "a11y",
    "508",
    "wcag",
    "aria"
  ],
  "author": {
    "name": "Curriculum Associates",
    "email": "opensource@cainc.com"
  },
  "homepage": "https://github.com/CurriculumAssociates/createjs-accessibility#readme",
  "license": "ISC",
  "main": "dist/main.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typedocMain": "src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CurriculumAssociates/createjs-accessibility.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist ./reports",
    "clobber": "npm run clean && rm -rf ./node_modules",
    "compile": "rollup -c --configPlugin typescript",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "start": "http-server -o ./dist/index.html"
  },
  "bugs": {
    "url": "https://github.com/CurriculumAssociates/createjs-accessibility/issues"
  },
  "dependencies": {
    "@curriculumassociates/accessibility-createjs-adapter": "^2.0.0",
    "createjs": "git+ssh://git@github.com:CreateJS/EaselJS.git",
    "gsap": "^3.11.3"
  },
  "devDependencies": {
    "@rollup/plugin-html": "^1.0.0",
    "http-server": "^14.1.1"
  }
}
