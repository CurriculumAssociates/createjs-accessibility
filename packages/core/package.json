{
  "name": "@curriculumassociates/accessibility-core",
  "version": "2.0.0",
  "description": "A module to allow for easy addition of accessibility",
  "keywords": [
    "accessibility",
    "a11y",
    "508",
    "wcag",
    "aria"
  ],
  "author": {
    "name": "Curriculum Associates",
    "email": "opensource@cainc.com"
  },
  "homepage": "https://github.com/CurriculumAssociates/createjs-accessibility#readme",
